<ContentDialog
    x:Class="WeatherDataAnalysis.View.DuplicateDayDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Title="Duplicate Day Found"
    PrimaryButtonText="Keep Old Data"
    SecondaryButtonText="Replace with New Data"
    PrimaryButtonClick="ContentDialog_PrimaryButtonClick"
    SecondaryButtonClick="ContentDialog_SecondaryButtonClick">
    
    <Grid>
        <StackPanel>
            <TextBlock  Name="conflictingDaysTextBlock" Text=""/>
            <CheckBox Name="duplicateDayCheckBox" Content="Do this for all files"  />
        </StackPanel>

    </Grid>
</ContentDialog>
